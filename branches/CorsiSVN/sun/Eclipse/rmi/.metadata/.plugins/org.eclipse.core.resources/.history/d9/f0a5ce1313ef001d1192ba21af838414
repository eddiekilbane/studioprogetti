package server.dao;

import java.rmi.Remote;
import java.util.Vector;

import server.beans.CD;

public interface DaoCdI extends Remote{

	public void insert (CD daInserire);
	public void update (CD daModificare);
	public void delete (CD daCancellare);
	public Vector<CD> getAll();
	public CD getById(Long id);
	
}
