import server.beans.CD;

public class ClientDao {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try{
	         
			Context namingContext = new InitialContext();
			//serverHello.Hello c1 = (serverHello.Hello) namingContext.lookup(url + "saluto");
			
			CD nuovo = new CD();
			nuovo.setArtist("Bob Dylan");
			nuovo.setTitle("Empire Burlesque");
			nuovo.setCompany("RCA");
			nuovo.setPrice(10.90);
			nuovo.setYear(1966);
			nuovo.setCountry("USA");
			nuovo.setQuantity(12);
			
	    
		}catch (Exception e){
		 
	        e.printStackTrace();
		}
	}

}
