<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM
			"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >


<hibernate-mapping package="model.easy" >

<class name="Bid"
	   table="BID"
	   lazy="true">

	<!-- Common id property. -->
	<id name="id"
		type="long"
		column="BID_ID"
	>
