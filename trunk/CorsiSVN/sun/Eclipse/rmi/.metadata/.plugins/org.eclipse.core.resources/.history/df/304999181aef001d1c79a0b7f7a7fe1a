<?xml version="1.0" encoding="UTF-8"?>
<project name="rmiTest" default="build" basedir=".">


	<property name="src.dir" value="./src" />
	<property name="build.dir" value="./build" />
	<property name="java.home" value="E:\Sun\SDK\jdk" />
	<property name="remote.classes" value="${build.dir}/server/dao/remote/impl" />


<target name="build">


		<javac srcdir="${src.dir}" destdir="${build.dir}" debug="on"
			verbose="yes" optimize="off" includes="*.class">
			<classpath> 
    			<pathelement path="${build.dir}"/>
    			<pathelement path="${java.home}/lib"/> 
  			</classpath> 
		</javac>

</target>


<target name="compileRmic">

<rmic 
base="./build"
classname="server.dao.remote.impl.DaoCdImplJdbc.class"
classpath="./bin"
/>
		


</target>



<target name="run">

		<java
			classname="com.dnsee.jpms2.aspects.deploy.DeployTableLogging"
			dir=".">
			
			<classpath refid="project.classpath" />
			<classpath>
				<pathelement location="build_deploy" />
			</classpath>

		</java>


</target>



</project>
