<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM
			"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >


<hibernate-mapping package="model.easy">

<class name="Item"
	   table="ITEM" lazy="true">

	<!-- Common id property. -->
	<id name="id"
		type="long"
		column="ITEM_ID">
		<generator class="native"/>
	</id>
</class>

</hibernate-mapping>